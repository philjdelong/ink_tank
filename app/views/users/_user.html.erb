
<%= form_for @user, method: method do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %><br>

  <%= f.label :tattoo_styles %>
  <%= f.text_field :tattoo_styles %><br>

  <%= f.label :price_per_hour %>
  <%= f.text_field :price_per_hour %><br>

  <%= f.label :bio %>
  <%= f.text_field :bio %><br>

  <%= submit_tag button_text %>
<% end %>

<!DOCTYPE html>
<html>
  <head>
    <title>InkTank</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% flash.each do |type, msg| %>
      <div>
        <%= msg %>
      </div>
    <% end %>

    <nav class="master_nav">

      <div class="logo">
        <%= link_to(image_tag('it_logo_bw.png', alt: "Ink Tank Logo"), "/") %>
      </div>
        <br>

      <div align='center' class="link">
        <%= link_to "Artists"%>
      </div>
        <br>

      <div align='center' class="link">
        <%= link_to "Locations"%>
      </div>
        <br>

      <div align='center' class="link">
        <%= link_to "Styles"%>
      </div>
        <br>

      <div align='center' class="link">
        <%= link_to "Login with Github", '/auth/github' %>
      </div>
        <br>

      <div align='center' class="link">
        <%= link_to "Register"%>
      </div>
        <br>
    </nav>
    <%= yield %>
  </body>
</html>

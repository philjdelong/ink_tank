<!DOCTYPE html>
<html>
  <head>
    <title>InkTank</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <nav class="master_nav">

      <div class="links">
        <%= link_to "Artists"%>**
        <%= link_to "Locations"%>**
        <%= link_to "Styles"%>**
        <%= link_to "Login"%>**
        <%= link_to "Register"%>
      </div> 

      <div class="logo">
        <%= link_to(image_tag('it_logo_bw.png', alt: "Ink Tank Logo"), "/") %>
      </div>
    </nav>

    <%= yield %>
  </body>
</html>
